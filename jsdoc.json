{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": "src/",
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "doc/",
    "template": "./node_modules/@otris/jsdoc-tsd",
    "recurse": true
  }
}